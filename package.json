{
    "name": "redux-minimal",
    "version": "1.0.0",
    "description": "Start building complex react-redux apps today, with this minimalist easy to understand starter kit (boilerplate)",
    "keywords": [
        "react",
        "redux",
        "minimal",
        "starter kit",
        "boilerplate"
    ],
    "main": "index.js",
    "scripts": {
        "start": "webpack --watch --config webpack.dev.config.js",
        "start-server": "./node_modules/.bin/nodemon server.js",
        "start-prod": "webpack --config webpack.prod.config.js",
        "start-prod-server": "./node_modules/.bin/pm2 start server.js",
        "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
        "test-watch": "npm test -- --watch"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "parserOptions": {
            "sourceType": "module",
            "allowImportExportEverywhere": false
        },
        "env": {
            "browser": true,
            "node": true,
            "mongo": true,
            "es6": true,
            "mocha": true,
            "commonjs": true
        },
        "rules": {
            "eqeqeq": "error",
            "curly": "error",
            "no-dupe-args": "error",
            "callback-return": "error"
        }
    },
    "babel": {
        "presets": [
            "es2015",
            "react",
            "stage-3"
        ]
    },
    "author": "Catalin Luntraru",
    "license": "MIT",
    "dependencies": {
        "autoprefixer": "^6.6.1",
        "babel-eslint": "^7.1.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-stage-0": "^6.16.0",
        "body-parser": "^1.15.2",
        "connect-redis": "^3.2.0",
        "cookie-parser": "^1.4.3",
        "dexie": "^1.5.1",
        "express": "^4.14.0",
        "express-limiter": "^1.6.0",
        "express-session": "^1.14.2",
        "helmet": "^3.4.0",
        "immutable": "^3.8.1",
        "jsonwebtoken": "^7.2.1",
        "localforage": "^1.4.3",
        "lodash": "^4.17.4",
        "material-ui": "^0.16.6",
        "mongoose": "^4.7.6",
        "morgan": "^1.7.0",
        "multer": "^1.2.1",
        "nodemon": "^1.11.0",
        "passport": "^0.3.2",
        "passport-facebook": "^2.1.1",
        "passport-github": "^1.1.0",
        "passport-google-oauth": "^1.0.0",
        "passport-local": "^1.0.0",
        "pm2": "^2.4.0",
        "precss": "^1.4.0",
        "react": "^15.4.1",
        "react-crop": "^4.0.2",
        "react-dom": "^15.4.1",
        "react-redux": "^4.4.6",
        "react-router": "^3.0.0",
        "react-router-redux": "^4.0.7",
        "react-swipeable-views": "^0.10.3",
        "react-tap-event-plugin": "^2.0.1",
        "redis": "^2.6.4",
        "redux": "^3.6.0",
        "redux-form": "^6.3.2",
        "redux-logger": "^2.7.4",
        "redux-saga": "^0.13.0",
        "socket.io": "^1.7.2",
        "socket.io-client": "^1.7.2",
        "superagent": "^3.3.2",
        "uuid": "^3.0.1",
        "webpack-dev-server": "^1.16.2"
    },
    "devDependencies": {
        "babel-core": "^6.20.0",
        "babel-loader": "^6.2.9",
        "babel-polyfill": "^6.20.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-stage-3": "^6.17.0",
        "babel-runtime": "^6.20.0",
        "clean-webpack-plugin": "^0.1.14",
        "css-loader": "^0.26.1",
        "enzyme": "^2.6.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "ignore-styles": "^5.0.1",
        "mocha": "^3.2.0",
        "node-sass": "3.7.0",
        "parallelshell": "^2.0.0",
        "react-addons-test-utils": "^15.4.1",
        "redux-freeze": "^0.1.5",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "webpack": "^1.14.0",
        "whatwg-fetch": "^2.0.1"
    }
}
